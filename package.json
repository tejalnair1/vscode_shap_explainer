{
  "name": "vscode-shap-explainer",
  "displayName": "FeatureSHAP Explainer",
  "description": "A VS Code extension that sends selected code to a local FeatureSHAP server and visualizes SHAP values.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:featureshap.computeShap"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "featureshap.computeShap",
        "title": "FeatureSHAP: Compute SHAP for Selection"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.4",
    "typescript": "^5.0.0",
    "@types/vscode": "^1.80.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
